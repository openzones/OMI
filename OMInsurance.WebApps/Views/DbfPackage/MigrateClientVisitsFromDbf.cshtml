@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <h2>Загрузка заявок для миграции филиалов</h2>
<div class="row">
    <form id="MigrateClientVisitsFromDbfParameters">
        <div class="col-md-8">
            <input id="MigrateClientVisitsFromDbf" class="btn btn-default" type="file" />
        </div>
        <div class="col-md-4">
            <input class="btn btn-default" id="uploadFileName"
                type="button"
                onclick="FileProcessor.uploadDbf('MigrateClientVisitsFromDbf', '/DbfPackage/MigrateClientVisitsFromDbf', 'report')"
                value="Загрузить заявки из DBF" />
        </div>
    </form>
</div>
<div class="row" id="report">
</div>

