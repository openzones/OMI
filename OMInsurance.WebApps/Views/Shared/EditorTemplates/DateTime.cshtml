@model DateTime ?
@{

    string Tabindex = "100";
    if (ViewData["tabindex"] != null)
    {
        if (ViewData.ContainsKey("tabindex"))
        {
            Tabindex = ViewData["tabindex"].ToString();
        }
    }
}
@Html.TextBoxFor(m => m,
Model.HasValue ? Model.Value.ToString("dd.MM.yyyy")
: String.Empty,
new { @class = "form-control datepicker", tabindex = Tabindex })