@model IEnumerable<OMInsurance.WebApps.Models.GoznakResponseModel>
@if(Model.Count() > 0){
<table class="table-hover table-striped table-condensed">
    <thead>
        <tr>
            <th style="width:5%">
                @Html.DisplayName("")
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.UnifiedPolicyNumber)
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.Lastname)
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.Firstname)
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.Secondname)
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.Birthday)
            </th>
            <th style="width:5%">
                @Html.DisplayNameFor(modelItem => modelItem.Sex)
            </th>
            <th style="width:5%">
                @Html.DisplayNameFor(modelItem => modelItem.DeliveryCenter)
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.TemporaryPolicyNumber)
            </th>
            <th style="width:10%">
                @Html.DisplayNameFor(modelItem => modelItem.UpdateDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr data-client-visit-group-id="@item.ClientVisitGroupId" data-response-id="@item.Id" class="responseRow">
                <td style="width:5%">
                    Goznak
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.UnifiedPolicyNumber)
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.Lastname)
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.Firstname)
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.Secondname)
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.Birthday)
                </td>
                <td style="width:5%">
                    @Html.DisplayFor(modelItem => item.Sex)
                </td>
                <td style="width:5%">
                    @Html.DisplayFor(modelItem => item.DeliveryCenter.Code)
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.TemporaryPolicyNumber)
                </td>
                <td style="width:10%">
                    @Html.DisplayFor(modelItem => item.UpdateDate)
                </td>
            </tr>
        }
    </tbody>
</table>
}
