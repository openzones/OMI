@model IEnumerable<OMInsurance.WebApps.Models.FundResponseUploadReportModel>
<table class="table-hover table-striped table-condensed">
    <thead>
        <tr>
            <th style="width: 10%">
                @Html.DisplayNameFor(model => model.DeliveryCenter)
            </th>
            <th style="width: 20%">
                @Html.DisplayNameFor(model => model.Fullname)
            </th>
            <th style="width: 10%">
                @Html.DisplayNameFor(model => model.PolicySeries)
            </th>
            <th style="width: 10%">
                @Html.DisplayNameFor(model => model.PolicyNumber)
            </th>
            <th style="width: 20%">
                @Html.DisplayNameFor(model => model.UnifiedPolicyNumber)
            </th>
            <th style="width: 5%">
                @Html.DisplayNameFor(model => model.Sex)
            </th>
            <th style="width: 10%">
                @Html.DisplayNameFor(model => model.Birthday)
            </th>
            <th style="width: 20%">
                @Html.DisplayNameFor(model => model.UploadResult)
            </th>
            <th style="width: 5%"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr data-clientvisitid="@item.ClientId">
                <td style="width: 10%">
                    @Html.DisplayFor(modelItem => item.DeliveryCenter.Code)
                </td>
                <td style="width: 20%">
                    @Html.DisplayFor(modelItem => item.Fullname)
                </td>
                <td style="width: 10%">
                    @Html.DisplayFor(modelItem => item.PolicySeries)
                </td>
                <td style="width: 10%">
                    @Html.DisplayFor(modelItem => item.PolicyNumber)
                </td>
                <td style="width: 20%">
                    @Html.DisplayFor(modelItem => item.UnifiedPolicyNumber)
                </td>
                <td style="width: 5%">
                    @Html.DisplayFor(modelItem => item.Sex)
                </td>
                <td style="width: 10%">
                    @Html.DisplayFor(modelItem => item.Birthday)
                </td>
                <th style="width: 20%">
                    @Html.DisplayFor(model => item.UploadResult)
                </th>
                <td style="width: 5%">
                    <a href="@Url.Action("Details", "ClientVisit", new { id = item.ClientVisitId })" target = "_blank" class="btn-default">
                        <span class="glyphicon glyphicon-pencil" />
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>

